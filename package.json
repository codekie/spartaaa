{
  "name": "spartaaa",
  "version": "0.2.0",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "babel-node src/server/index.js",
    "test": "cross-env LOG_LEVEL=error jest --roots=src --config=jest.config.js",
    "coverage": "cross-env LOG_LEVEL=error yarn run coverage:run",
    "coverage-html": "cross-env LOG_LEVEL=error yarn run coverage:run --coverageReporters=html --coverageDirectory=coverage",
    "coverage-lcov": "cross-env LOG_LEVEL=error yarn run coverage:run --coverageReporters=lcov --coverageDirectory=coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "yarn run clean-build && yarn run build:webpack",
    "clean-build": "rimraf dist",
    "release": "npm run build && npm run release:stage-artifacts && standard-version -a",
    "start-dev": "concurrently -c \"magenta.bold,cyan.bold\" -n \"server, webpack\" \"yarn run start-dev:server-watch\" \"yarn run start-dev:webpack\"",
    "start-dev-debug": "concurrently -c \"magenta.bold,cyan.bold\" -n \"server, webpack\" \"yarn run start-dev:server-watch-debug\" \"yarn run start-dev:webpack\"",
    "coverage:run": "jest --roots=src --coverage --collectCoverageFrom=src/**/*.{js,jsx,mjs}",
    "release:stage-artifacts": "git add dist/*",
    "start-dev:server-watch": "nodemon --watch \"src\" --watch \"config\" --ignore \"src/client\" --exec yarn run start-dev:server",
    "start-dev:server-watch-debug": "nodemon --watch \"src\" --watch \"config\" --ignore \"src/client\" --exec yarn run start-dev:server-debug",
    "start-dev:server": "babel-node ./src/server/index.js",
    "start-dev:server-debug": "babel-node --inspect ./src/server/index.js",
    "start-dev:webpack": "webpack-dev-server --mode development --hot --history-api-fallback --inline --progress --config ./config/webpack/dev.js",
    "build:webpack": "webpack --mode production --bail --progress --config ./config/webpack/build.js"
  },
  "repository": "https://github.com/codekie/spartaaa.git",
  "author": "Code Kie <codekie@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "react-test-renderer": "^16.6.2",
    "rimraf": "^2.6.3",
    "sass": "^1.16.0",
    "sass-loader": "^7.1.0",
    "standard-version": "^4.4.0",
    "style-loader": "^0.23.1",
    "supertest": "^3.4.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bulma-extensions": "^4.0.1",
    "bulmaswatch": "^0.7.2",
    "chokidar": "^3.0.2",
    "colors": "^1.3.3",
    "express": "^4.16.4",
    "express-request-id": "^1.4.1",
    "immutable": "^3.8.2",
    "lodash": "^4.17.14",
    "memoize-one": "^5.0.0",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "queueing-subject": "^0.3.0",
    "react": "^16.6.2",
    "react-bulma-components": "^3.0.1-3",
    "react-dom": "^16.6.2",
    "react-hot-loader": "^4.6.3",
    "react-redux": "6.0.0",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "snapshot-diff": "^0.5.0",
    "winston": "^3.1.0",
    "ws": "^6.1.2"
  }
}

{
  "name": "spartaaa",
  "version": "0.1.0",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "babel-node src/server/index.js",
    "test": "jest --roots=src",
    "coverage": "yarn run coverage:run",
    "coverage-html": "yarn run coverage:run --coverageReporters=html --coverageDirectory=coverage",
    "coverage-lcov": "yarn run coverage:run --coverageReporters=lcov --coverageDirectory=coverage",
    "coverage:run": "jest --roots=src --coverage --collectCoverageFrom=src/**/*.{js,jsx,mjs}",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "yarn run clean-build && yarn run build:webpack",
    "clean-build": "rimraf dist",
    "start-dev": "concurrently -c \"magenta.bold,cyan.bold\" -n \"server, webpack\" \"yarn run start-dev:server-watch\" \"yarn run start-dev:webpack\"",
    "start-dev:server-watch": "nodemon --ignore \"src/client\" --exec yarn run start-dev:server",
    "start-dev:server": "babel-node $NODE_DEBUG_OPTION ./src/server/index.js",
    "start-dev:webpack": "webpack-dev-server --mode development --hot --history-api-fallback --inline --progress --config ./config/webpack/dev.js",
    "build:webpack": "webpack --mode production --bail --progress --config ./config/webpack/build.js"
  },
  "repository": "https://github.com/codekie/spartaaa.git",
  "author": "Code Kie <codekie@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.0.1",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "sass": "^1.14.1",
    "sass-loader": "^7.1.0",
    "standard-version": "^4.4.0",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "bulma-extensions": "^3.0.0",
    "bulmaswatch": "^0.7.1",
    "chokidar": "^2.0.4",
    "colors": "^1.3.2",
    "express": "^4.16.3",
    "express-request-id": "^1.4.1",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "queueing-subject": "^0.3.0",
    "react": "^16.5.2",
    "react-bulma-components": "^2.2.0",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "winston": "^3.1.0",
    "ws": "^6.1.0"
  }
}

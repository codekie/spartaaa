{
  "name": "spartaaa",
  "version": "0.1.0",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "babel-node src/server/index.js",
    "build": "yarn run clean-build && yarn run build:webpack",
    "clean-build": "rimraf dist",
    "start-dev": "concurrently -c \"magenta.bold,cyan.bold\" -n \"server, webpack\" \"yarn run start-dev:server-watch\" \"yarn run start-dev:webpack\"",
    "start-dev:server-watch": "nodemon --exec yarn run start-dev:server",
    "start-dev:server": "babel-node $NODE_DEBUG_OPTION ./src/server/index.js",
    "start-dev:webpack": "webpack-dev-server --mode development --hot --history-api-fallback --inline --progress --config ./config/webpack/dev.js",
    "build:webpack": "webpack --mode production --bail --progress --config ./config/webpack/build.js"
  },
  "repository": "https://github.com/codekie/spartaaa.git",
  "author": "Code Kie <codekie@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "standard-version": "^4.4.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
